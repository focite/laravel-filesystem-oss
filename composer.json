{
    "name": "focite/laravel-filesystem-oss",
    "description": "aliyun oss filesystem storage for laravel 5+",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aliyuncs/oss-sdk-php": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Focite\\Flysystem\\OSS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Focite\\Flysystem\\OSS\\AliOssServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
